{
  "name": "@maxlz/nestjs-cacheable",
  "version": "0.2.5",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "MAXLZ",
  "license": "MIT",
  "description": "Using Cacheable as a caching tool in Nestjs.",
  "keywords": [
    "nestjs",
    "cache",
    "cacheable",
    "CacheModule",
    "decorator",
    "etcd",
    "redis",
    "valkey"
  ],
  "homepage": "https://github.com/MAXLZ1/nestjs-cacheable/#readme",
  "bugs": "https://github.com/MAXLZ1/nestjs-cacheable/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/MAXLZ1/nestjs-cacheable.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "LICENSE"
  ],
  "scripts": {
    "prepare": "husky",
    "build": "rimraf ./dist && tsc -p tsconfig.build.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:e2e": "vitest run --config test/e2e/vitest.config.e2e.mts",
    "link": "eslint . --cache",
    "format": "prettier . --check --cache"
  },
  "packageManager": "pnpm@9.15.0",
  "dependencies": {
    "@keyv/memcache": "^2.0.1",
    "keyv": "^5.3.2",
    "lru-cache": "^11.0.2"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0 || ^11.0.0",
    "@nestjs/core": "^10.0.0 || ^11.0.0",
    "cacheable": "^1.8.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/js": "^9.23.0",
    "@keyv/etcd": "^2.1.0",
    "@keyv/mongo": "^3.0.1",
    "@keyv/mysql": "^2.1.0",
    "@keyv/postgres": "^2.1.0",
    "@keyv/redis": "^4.3.2",
    "@keyv/sqlite": "^4.0.1",
    "@keyv/valkey": "^1.0.3",
    "@nestjs/common": "^11.0.11",
    "@nestjs/core": "^11.0.11",
    "@nestjs/platform-express": "^11.0.11",
    "@nestjs/testing": "^11.0.11",
    "@swc/core": "^1.11.16",
    "@types/supertest": "^6.0.2",
    "@vitest/coverage-v8": "^3.0.7",
    "cacheable": "^1.8.9",
    "dotenv-flow": "^4.1.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "prettier": "^3.5.3",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "supertest": "^7.0.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.29.0",
    "unplugin-swc": "^1.5.1",
    "vitest": "^3.0.7"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ]
  }
}
